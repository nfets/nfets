{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": "src",
    "composite": true,
    "paths": {
      "@nfets/nfe": ["./src/index"],
      "@nfets/nfe/*": ["./src/*"],
      "@nfets/core": ["../core/dist/index"],
      "@nfets/core/*": ["../core/dist/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["test/**/*.ts"]
}
